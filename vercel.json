{
    "version": 2,
    "builds": [
        {
            "src": "./src/app.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/weather",
            "dest": "./src/app.js"
        },
        {
            "src": "/help",
            "dest": "./src/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "./src/app.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}