{
    "version": 2,
    "builds": [
        {
            "src": "./src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/weather",
            "dest": "./src/app.js"
        },
        {
            "src": "/help",
            "dest": "./src/app.js"
        },
        {
            "src": "/.*",
            "dest": "./src/app.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}